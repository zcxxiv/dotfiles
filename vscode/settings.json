{
  "editor.fontFamily": "PragmataPro",
  "workbench.fontAliasing": "auto",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.rulers": [
    80
  ],
  "editor.lineNumbers": "relative",
  "workbench.activityBar.visible": true,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "phase",
  "workbench.iconTheme": "vs-seti",
  "workbench.sideBar.location": "left",
  "workbench.colorCustomizations": {
    "[Hop Light]": {
      "editor.lineHighlightBackground": "#f9f9f9",
      "editorCursor.foreground": "#729ceb",
      "editorCursor.background": "#FFFFFF",
      "terminal.ansiBlack": "#41a6d9",
      "terminal.ansiBlue": "#41a6d9",
      "terminal.ansiBrightBlack": "#828C99",
      "terminal.ansiBrightBlue": "#81c4e6",
      "terminal.ansiBrightCyan": "#85d3b9",
      "terminal.ansiBrightGreen": "#9ed106",
      "terminal.ansiBrightMagenta": "#C9b1e1",
      "terminal.ansiBrightRed": "#FF8080",
      "terminal.ansiBrightWhite": "#d9d8d7",
      "terminal.ansiBrightYellow": "#f6b860",
      "terminal.ansiCyan": "#85d3b9",
      "terminal.ansiGreen": "#9acc06",
      "terminal.ansiMagenta": "#a37acc",
      "terminal.ansiRed": "#ff3333",
      "terminal.ansiWhite": "#ffffff",
      "terminal.ansiYellow": "#f29718",
      "scrollbarSlider.background": "#f7f7f7",
      "scrollbarSlider.hoverBackground": "#f3f3f3",
      "activityBar.background": "#f6f6f6",
      "activityBar.foreground": "#828c99",
      "sideBar.background": "#fefefe",
      "sideBar.border": "#eeeeee",
      "sideBarSectionHeader.background": "#f6f6f6",
      "list.inactiveSelectionBackground": "#f6f6f6",
      "tab.inactiveBackground": "#f6f6f6",
      "editorGroupHeader.tabsBackground": "#f6f6f6",
      "panel.border": "#eeeeee",
      "editorLineNumber.foreground": "#cfcfcf",
      "titleBar.activeBackground": "#efefef",
      "editor.selectionBackground": "#b1e0fa9f",
      "editorBracketMatch.border": "#9ed106",
      "statusBar.background": "#416dbe",
      "statusBar.noFolderBackground": "#005f5f",
      "statusBar.debuggingBackground": "#639958"
    },
    "[Nord]": {
      "editorCursor.foreground": "#b7d49d",
      "terminalCursor.foreground": "#b7d49d",
    },
    "[Verdandi Alter]": {
      "activityBarBadge.background": "#58B560",
      "editor.background": "#161616",
      "activityBar.background": "#121212",
      "titleBar.activeBackground": "#161616",
      "editorCursor.foreground": "#58B560",
      "editorCursor.background": "#FFFFFF",
      "terminalCursor.foreground": "#58B560",
      "statusBar.debuggingBackground": "#58B560",
      "statusBar.foreground": "#FEFEFE",
      "statusBar.debuggingForeground": "#FEFEFE"
    },
    "[City Lights]": {
      "editorCursor.foreground": "#88C0D0",
      "editorSuggestWidget.selectedBackground": "#3F4D5B",
      "activityBarBadge.background": "#88C0D0",
      "statusBar.debuggingBackground": "#70a9b8",
      "statusBar.debuggingForeground": "#fefefe"
    },
    "[Default Dark+]": {
      "activityBarBadge.background": "#58B560",
      "editor.background": "#161616",
      "activityBar.background": "#121212",
      "titleBar.activeBackground": "#161616",
      "editorCursor.foreground": "#58B560",
      "editorCursor.background": "#FFFFFF",
      "terminalCursor.foreground": "#58B560",
      "statusBar.debuggingBackground": "#58B560",
    }
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "workbench.statusBar.visible": true,
  "editor.renderLineHighlight": "all",
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.enableNeovim": true,
  // "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.otherModesKeyBindings": [
    {
      "before": [
        "c",
        "c"
      ],
      "after": [
        "d",
        "d",
        "d",
        "O"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.easymotionMarkerHeight": 19,
  "vim.easymotionMarkerFontFamily": "PragmataPro",
  "vim.easymotionMarkerFontSize": "14",
  "vim.easymotionMarkerYOffset": 5,
  // "vim.easymotionMarkerBackgroundColor": "#FFFFFF",
  // "vim.easymotionMarkerForegroundColorOneChar": "#FF8080",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "prettier.singleQuote": true,
  "tslint.alwaysShowRuleFailuresAsWarnings": true,
  "debug.hideActionBar": true,
  "prettier.trailingComma": "es5",
  "window.zoomLevel": 0,
  "workbench.list.openMode": "doubleClick",
  "gitlens.codeLens.enabled": false,
  "workbench.colorTheme": "Hop Light",
  "gitlens.keymap": "alternate",
}