    "terminal.ansiBrightBlue": "#81c4e6",
    "terminal.ansiBrightCyan": "#85d3b9",
    "terminal.ansiBrightGreen": "#9ed106",
    "terminal.ansiBrightMagenta": "#C9b1e1",
    "terminal.ansiBrightRed": "#FF8080",
    "terminal.ansiBrightWhite": "#d9d8d7",
    "terminal.ansiBrightYellow": "#f6b860",
    "terminal.ansiCyan": "#85d3b9",
    "terminal.ansiGreen": "#9acc06",
